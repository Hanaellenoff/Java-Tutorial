import java.util.Scanner;

public class IfElse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("Enter a grade:");
        int n = scan.nextInt();
        System.out.println(n);
        
        if (n <= 90 && n <= 100) {
    		System.out.println("A");
	   }
	}
}
